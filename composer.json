{
    "name": "dstotijn/yii2-json-schema-validator",
    "description": "A Yii2 extension that provides a validator class for JSON Schema validation.",
    "keywords": ["yii2", "json", "schema", "validator"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dstotijn/yii2-json-schema-validator/issues",
        "source": "https://github.com/dstotijn/yii2-json-schema-validator"
    },
    "authors": [
        {
            "name": "David Stotijn",
            "email": "dstotijn@gmail.com"
        }
    ],
    "require": {
        "justinrainbow/json-schema": "~1.3",
        "yiisoft/yii2": "2.*",
        "scrutinizer/ocular": "^1.1"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/codeception": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "dstotijn\\yii2jsv\\": "src/",
            "dstotijn\\yii2jsv\\tests\\": "tests/",
            "dstotijn\\yii2jsv\\tests\\data\\models\\": "tests/data/models/"
        }
    }
}
